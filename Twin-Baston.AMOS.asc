               D$=Str$(DEPART)-" "
               If Len(D$)=1
                  D$="0"+D$
               End If 
               I1=Val(Left$(D$,1))+16
               I2=Val(Right$(D$,1))+16
               Bob Clear 
               Paste Bob 253,63,I1
               Paste Bob 274,63,I2
               Bob Draw 
            End If 
         Else 
            Proc NOUVELLE_CASE
         End If 
         Repeat 
            Wait Vbl 
            J=Joy(1)
            If(Key State(76)=True) and(Key State(77)=False)
               J=1
            End If 
            If(Key State(77)=True) and(Key State(76)=False)
               J=2
            End If 
            If(Key State(79)=True) and(Key State(78)=False)
               J=4
            End If 
            If(Key State(78)=True) and(Key State(79)=False)
               J=8
            End If 
            If Key State(64)=True
               J=16
            End If 
         Until J=0
      End If 
      KK$=Inkey$
      If KK$="+"
         If CASE=5
            Gosub ZIKVP
         End If 
         If CASE=6
            Gosub SFXVP
         End If 
         If CASE=7
            Gosub JEUXVP
         End If 
      End If 
      If KK$="-"
         If CASE=5
            Gosub ZIKVM
         End If 
         If CASE=6
            Gosub SFXVM
         End If 
         If CASE=7
            Gosub JEUXVM
         End If 
      End If 
   Until FINISHED
   '--------------
   For I=ZIKVOL To 0 Step -1
      Volume I
      Wait Vbl 
   Next I
   MUSIQUE_OFF
   Erase 3
   Auto View Off 
   Bob Off 
   
   Bank Swap 1,12
   
   Screen Close 2
   Screen Close 1
   Screen Close 0
   Screen Open 0,320,256,32,0
   Screen Display 0,128,40,320,256
   Flash Off 
   Curs Off 
   Hide On 
   Get Sprite Palette 
   Cls 0
   Double Buffer 
   Autoback 1
   Wait Vbl 
   Auto View On 
   View 
   Pop Proc
   '
   ZIKVM:
   If ZIKVOL>0
      Dec ZIKVOL
      Volume ZIKVOL
      Ink 5
      Draw 65+ZIKVOL,10 To 65+ZIKVOL,23
   End If 
   Return 
   ZIKVP:
   If ZIKVOL<63
      Inc ZIKVOL
      Volume ZIKVOL
      Ink 9
      Draw 64+ZIKVOL,10 To 64+ZIKVOL,23
   End If 
   Return 
   SFXVM:
   If SFXVOL>0
      Dec SFXVOL
      Ink 5
      Draw 65+SFXVOL,46 To 65+SFXVOL,59
   End If 
   Return 
   SFXVP:
   If SFXVOL<63
      Inc SFXVOL
      Ink 9
      Draw 64+SFXVOL,46 To 64+SFXVOL,59
   End If 
   Return 
   JEUXVM:
   If JEUXVOL>0
      Dec JEUXVOL
      Ink 5
      Draw 65+JEUXVOL,82 To 65+JEUXVOL,95
   End If 
   Return 
   JEUXVP:
   If JEUXVOL<63
      Inc JEUXVOL
      Ink 9
      Draw 64+JEUXVOL,82 To 64+JEUXVOL,95
   End If 
   Return 
End Proc
Procedure HALL_OF_FAME
   Shared HISCORE(),HISCORE$(),HIMODE(),MSG_SCORES,CLI1,CLI2
   Autoback 0
   Flash Off 
   Palette 0,0,0,0,0
   Cls 0
   _MESSAGE[MSG_SCORES,0]
   Ink 1,0
   For I=1 To 10
      H$=Str$(HISCORE(I))-" "
      S$=Right$(Str$(I),2)
      If I=1
         S$=S$+"ST"
      Else 
         If I=2
            S$=S$+"ND"
         Else 
            If I=3
               S$=S$+"RD"
            Else 
               S$=S$+"TH"
            End If 
         End If 
      End If 
      If HIMODE(I)=False
         S$=S$+" A "
      Else 
         S$=S$+" B "
      End If 
      S$=S$+String$("0",6-Len(H$))+H$+"  "+HISCORE$(I)
      If(I=CLI1) or(I=CLI2)
         Ink 4
      Else 
         Ink 1
      End If 
      Text 16,12*I+80,S$
   Next I
   Screen Copy Logic(0) To Physic(0)
   Autoback 2
End Proc
