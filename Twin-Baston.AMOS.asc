               If X=15
                  X=1
                  Inc Y
                  Exit If Y=15,2
               End If 
               C2(X,Y)=0
            Next J
         Else 
            C2(X,Y)=C
            If C=43
               Inc NBTRESORS
            End If 
            Inc X
            If X=15
               X=1
               Inc Y
            End If 
         End If 
      Next I
      Input #1,DUMMY
      
      Input #1,LEVEL2
      ' Remettre sécurité avant de diffuser
      LEVEL2=LEVEL
      If LEVEL2<>LEVEL
         FIN
      End If 
      
      Input #1,YAITEM
      If YAITEM
         Input #1,ITEM
         Input #1,ITEMX
         Input #1,ITEMY
      End If 
      If PARTIE=1
         Input #1,MXTEMPS(1)
         Input #1,MXTEMPSTOTAL
         Input #1,NBMONSTRES
         Input #1,COM$
         Centre COM$
         Input #1,XD1
         Input #1,YD1
         Input #1,IMGBACK
      Else 
         Input #1,MXTEMPS(1)
         Input #1,MXTEMPS(2)
         Input #1,MEMETEMPS
         Input #1,MXTEMPSTOTAL
         Input #1,NBMONSTRES
         Input #1,COM$
         Centre COM$
         Input #1,XD1
         Input #1,YD1
         Input #1,XD2
         Input #1,YD2
         Input #1,IMGBACK
      End If 
      Input #1,BONUSDELAI
      Input #1,BONUSRESTE
      If NBMONSTRES>0
         For I=1 To NBMONSTRES
            Input #1,MX(I)
            Input #1,MY(I)
            Input #1,NBSQ(I)
            If NBSQ(I)>0
               For J=1 To NBSQ(I)
                  Input #1,MA(I,J)
                  Input #1,MH(I,J)
                  Input #1,MV(I,J)
                  Input #1,MT(I,J)
               Next J
            End If 
         Next I
      End If 
      ' Bonusmachin
   Else 
      Centre "Plus de tableaux !!"
      FIN
   End If 
   Close 1
   MXTEMPSTOTAL=MXTEMPSTOTAL*50
End Proc
