Procedure JEU7
   ' Jeu de butage/butation de Lemmings pour TB 
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,SFXVOL,TBLB
   
   ' On mémorise l'état précédent 
   Dim P(31)
   For I=0 To 31
      P(I)=Colour(I)
   Next I
   Bank Swap 1,4
   
   ' On s'installe
   _LOAD["Twin2:Samples-Lemmings.Abk",6]
   Sam Bank 6
   _LOAD["Twin2:Sprites-Lemmings.Abk",-1]
   Screen Open 0,320,256,8,Lowres
   Screen Display 0,128,40,320,256
   Curs Off 
   Flash Off 
   Cls 0
   Double Buffer 
   Get Sprite Palette 
   Priority Reverse On 
   Volume SFXVOL
   '
   Amal Off 
   Bob Off 
   Autoback 1
   Synchro Off 
   '
   Bob Clear 
   Wait Vbl 
   Cls 0
   Screen Swap 
   Bob Clear 
   Wait Vbl 
   Cls 0
   Ink 1,0
   Text 40,7," PLAYER 1 = 0"
   Text 162,7," PLAYER 2 = 0"
   '
   Dim X(1),Y(1),BUTOS(1),F(1)
   '
   X(1)=50
   Y(1)=100
   X(0)=260
   Y(0)=100
   BUTOS(0)=0
   BUTOS(1)=0
   '
   ' Viseurs
   Bob 1,X(1),Y(1),1
   Bob 2,X(0),Y(0),3
   Channel 1 To Bob 1
   Channel 2 To Bob 2
   Amal 1,"Loop: Pause; If BC(1,3,10) then Jump Coll; Let A=1; Jump Loop; Coll: Let A=2; Jump Loop;"
   Amal 2,"Loop: Pause; If BC(2,3,10) then Jump Coll; Let A=3; Jump Loop; Coll: Let A=4; Jump Loop;"
   '
   ' Lemmings 
   NBLEM=8
   For BITOS=3 To 2+NBLEM
      Bob BITOS,Rnd(32)*10,10,5
      Channel BITOS To Bob BITOS
      Channel BITOS+NBLEM To Bob BITOS
      Amal BITOS,"Anim 0,(5,5)(6,5)(7,5)(6,5);"
      V=(Rnd(1)+1)*2
      Amal BITOS+NBLEM,"Move 0,256,"+Str$(256/V)+";"
   Next BITOS
   '
   Amal On 
   Timer=0
   '
   Repeat 
      For I=0 To 1
         If(Jleft(I)) or((I=0) and(Key State(79)))
            Add X(I),-2,0 To 304
         End If 
         If(Jright(I)) or((I=0) and(Key State(78)))
            Add X(I),2,0 To 304
         End If 
         If(Jup(I)) or((I=0) and(Key State(76)))
            Add Y(I),-2,0 To 240
         End If 
         If(Jdown(I)) or((I=0) and(Key State(77)))
            Add Y(I),2,0 To 240
         End If 
         If(Fire(I)) or((I=0) and(Key State(64)))
            If F(I)=False
               Sam Play 4*(I+1),1
               If Bob Col(-I+2,3 To 2+NBLEM)
                  For ZOB=3 To 2+NBLEM
                     If Col(ZOB)
                        LEM=ZOB
                     End If 
                  Next ZOB
                  If I Bob(LEM)<>8
                     Sam Play 3,2
                     Inc BUTOS(I)
                     Amal Freeze LEM
                     Bob LEM,,,8
                     Text 40,7," PLAYER 1 ="+Str$(BUTOS(1))+" "
                     Text 162,7," PLAYER 2 ="+Str$(BUTOS(0))+" "
                  End If 
               End If 
               F(I)=True
            End If 
         Else 
            F(I)=False
         End If 
      Next I
      '
      For BITOS=3 To 2+NBLEM
         If Chanmv(BITOS+NBLEM)=False
            Bob BITOS,Rnd(32)*10,10,5
            V=(Rnd(1)+1)*2
            Amal BITOS+NBLEM,"Move 0,256,"+Str$(256/V)+";"
            Amal On BITOS
            Amal On BITOS+NBLEM
         End If 
      Next BITOS
      '
      Bob 1,X(1),Y(1),
      Bob 2,X(0),Y(0),
      Synchro 
      Wait Vbl 
      '
   Until Timer>=1100
   '
   If BUTOS(1)>BUTOS(0)
      GAGNE=1
   Else 
      If BUTOS(1)<BUTOS(0)
         GAGNE=2
      Else 
         GAGNE=0
      End If 
   End If 
   '
   Wait 40
   Fade 1
   Bob Clear 
   Amal Off 
   Bob Off 
   Wait Vbl 
   Cls 0
   Screen Swap 
   Cls 0
   
   Bank Swap 1,4
   Erase 4
   
   Erase 6
   Sam Bank 5
   
   SP[4]
   If GAGNE=0
      _MESSAGE[MSG_EGALITE,0]
      Add SCORE(1),5000
      Add SCORE(2),5000
   Else 
      TBLB=True
      If GAGNE=1
         _MESSAGE[MSG_WINNER,1]
         Add SCORE(1),10000
      Else 
         _MESSAGE[MSG_WINNER,2]
         Add SCORE(2),10000
      End If 
   End If 
   TBLB=False
   Wait 20
   Volume 0
   Synchro Off 
End Proc
