Procedure JEU6
   ' Pak Man... 
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,TBLB
   Volume JEUXVOL
   
   Dim CA(20,16)
   Dim X(1),Y(1),INV(1),SC(1),F(1),FX(1),FY(1)
   Dim M$(1,8)
   
   ' On mémorise l'état précédent 
   Dim P(31)
   For I=0 To 31
      P(I)=Colour(I)
   Next I
   Bank Swap 1,4
   
   ' On s'installe
   _LOAD["Twin2:Samples-PakMan.Abk",6]
   Sam Bank 6
   _LOAD["Twin2:Sprites-PakMan.Abk",-1]
   Screen Open 0,320,256,8,Lowres
   Screen Display 0,128,40,320,256
   Palette 0,0,0,0,0,0,0,0
   Curs Off 
   Flash Off 
   Priority Reverse On 
   '
   Amal Off 
   Bob Off 
   Bob Clear 
   Wait Vbl 
   Cls 0
   '
   Autoback 1
   Synchro On 
   
   ' Le terrain 
   NBPAS=0
   BOUBOUF=0
   Restore DT
   For Y=0 To 15
      For X=0 To 19
         Read CA(X,Y)
         X2=X*16
         Y2=Y*16
         If CA(X,Y)=1
            Ink 1
            Bar X2,Y2 To X2+15,Y2+15
         Else 
            Ink 7
            If CA(X,Y)=0
               Draw X2+7,Y2+6 To X2+7,Y2+8
               Draw X2+6,Y2+7 To X2+8,Y2+7
               Inc NBPAS
            Else 
               If CA(X,Y)=3
                  Circle X2+7,Y2+7,3
                  Paint X2+7,Y2+7
               End If 
            End If 
         End If 
      Next X
   Next Y
   Ink 7,1
   Text 246,18,"TWIN PAK"
   
   ' Zou! 
   Get Sprite Palette 
   Colour 7,$EF0
   View 
   Wait Vbl 
   
   ' P'tit'zinits 
   X(1)=5
   Y(1)=14
   X(0)=11
   Y(0)=14
   SC(0)=0
   SC(1)=0
   FX(0)=1
   FY(0)=1
   FX(1)=18
   FY(1)=2
   Bob 1,X(0)*16,Y(0)*16,7
   Bob 2,X(1)*16,Y(1)*16,1
   Bob 3,FX(0)*16,FY(0)*16,5
   Bob 4,FX(1)*16,FY(1)*16,6
   For I=1 To 4
      Channel I To Bob I
   Next I
   M$(1,1)="LA=3;M0,-16,4;"
   M$(1,2)="LA=4;M0,16,4;"
   M$(1,4)="LA=2;M-16,0,4;"
   M$(1,8)="LA=1;M16,0,4;"
   M$(0,1)="LA=9;M0,-16,4;"
   M$(0,2)="LA=10;M0,16,4;"
   M$(0,4)="LA=8;M-16,0,4;"
   M$(0,8)="LA=7;M16,0,4;"
   
   ' Ze boucle
   OVER=False
   Repeat 
      
      ' Ze players 
      For I=0 To 1
         If F(I)=False
            NX=X(I)
            NY=Y(I)
            J=Joy(I)
            If I=0
               If(Key State(76)=True) and(Key State(77)=False)
                  J=1
               End If 
               If(Key State(77)=True) and(Key State(76)=False)
                  J=2
               End If 
               If(Key State(79)=True) and(Key State(78)=False)
                  J=4
               End If 
               If(Key State(78)=True) and(Key State(79)=False)
                  J=8
               End If 
            End If 
            If J=1
               Dec NY
            End If 
            If J=2
               Inc NY
            End If 
            If J=4
               Dec NX
            End If 
            If J=8
               Inc NX
            End If 
            If(NX<>X(I)) or(NY<>Y(I))
               ' Pas mur ?
               If CA(NX,NY)<>1
                  X(I)=NX
                  Y(I)=NY
                  ' Pas vide ? 
                  If CA(NX,NY)<>2
                     Add SC(I),50
                     Bob Clear 
                     Ink 0
                     Bar NX*16+4,NY*16+4 To NX*16+10,NY*16+10
                     Bob Draw 
                     If CA(NX,NY)=3
                        ' Pac-gum
                        Sam Play %11,1
                        If I=1
                           Flash 2,"(FF0,10)(F00,10)"
                        Else 
                           Flash 6,"(FA0,10)(F00,10)"
                        End If 
                        INV(I)=70
                     Else 
                        ' Pastille 
                        Sam Play %1100,3
                        Inc BOUBOUF
                        If BOUBOUF=NBPAS
                           OVER=True
                        End If 
                     End If 
                     CA(NX,NY)=2
                  End If 
                  Amal I+1,M$(I,J)
                  Amal On I+1
               End If 
            End If 
         End If 
      Next I
      
      ' Ze ghosts
      For ZZ=0 To 1
         FX=FX(ZZ)
         FY=FY(ZZ)
         D0=(FX-X(0))*(FX-X(0))+(FY-Y(0))*(FY-Y(0))
         D1=(FX-X(1))*(FX-X(1))+(FY-Y(1))*(FY-Y(1))
         If D0<=D1
            PP=0
         Else 
            PP=1
         End If 
         If F(0)=True
            PP=1
         End If 
         If F(1)=True
            PP=0
         End If 
         BLK=0
         Repeat 
            Inc BLK
            If BLK>4
               RX=Rnd(2)-1
               If RX=0
                  RY=Rnd(1)*2-1
               Else 
                  RY=0
               End If 
               NX=FX+RX
               NY=FY+RY
            Else 
               NX=FX
               NY=FY
               R=Rnd(1)
               MOINS=-1
               PLUS=1
               If INV(PP)>0
                  Swap MOINS,PLUS
               End If 
               If(R=0) and(X(PP)<>FX)
                  If X(PP)<FX
                     NX=FX+MOINS
                  Else 
                     NX=FX+PLUS
                  End If 
               Else 
                  If Y(PP)<FY
                     NY=FY+MOINS
                  Else 
                     NY=FY+PLUS
                  End If 
               End If 
            End If 
         Until(CA(NX,NY)<>1)
         LVX=NX-FX
         LVY=NY-FY
         FX=NX
         FY=NY
         Amal 3+ZZ,"M"+Str$(LVX*16)+","+Str$(LVY*16)+",4;"
         Amal On 3+ZZ
         FX(ZZ)=FX
         FY(ZZ)=FY
      Next ZZ
      
      ' Touché un fantôme ?
      For J=0 To 1
         For I=0 To 1
            If(FX(J)=X(I)) and(FY(J)=Y(I)) and(INV(I)=0)
               Bob Off I+1
               F(I)=True
               If F(0)=F(1)
                  OVER=True
               End If 
            End If 
         Next I
      Next J
      
      ' Bouffe l'autre 
      If(X(0)=X(1)) and(Y(0)=Y(1))
         If(INV(1)>0) and(F(0)=False) and(INV(0)=0)
            Bob Off 1
            F(0)=True
            Sam Play %11,2
         End If 
         If(INV(0)>0) and(F(1)=False) and(INV(1)=0)
            Bob Off 2
            F(1)=True
            Sam Play %11,2
         End If 
      End If 
      
      ' Tempo
      Wait 4
      For I=0 To 1
         If INV(I)>0
            Dec INV(I)
            If INV(I)=0
               Flash Off 
               Colour 2,$FF0
               Colour 6,$FA0
               If INV(0)>0
                  Flash 6,"(FA0,10)(F00,10)"
               Else 
                  If INV(1)>0
                     Flash 2,"(FF0,10)(F00,10)"
                  End If 
               End If 
            End If 
         End If 
      Next I
      
   Until OVER
   
   Bob Update On 
   Flash Off 
   Fade 1
   Wait 15
   Cls 0
   Erase 6
   Sam Bank 5
   SP[2]
   TBLB=True
   Add SCORE(1),SC(1)
   Add SCORE(2),SC(0)
   If SC(1)>SC(0)
      _MESSAGE[MSG_WINNER,1]
      Add SCORE(1),10000
   Else 
      _MESSAGE[MSG_WINNER,2]
