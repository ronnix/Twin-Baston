Procedure CHARGER[N]
   Shared C(),MN(),MX(),MY(),MV()
   Open Random 1,"Twin:NewLevels.Def"
   Field 1,1 As CASE$
   INDEX=(N-1)*196+1
   For X=1 To 14
      For Y=1 To 14
         Get 1,INDEX
         C(X,Y)=Val(CASE$)
         Inc INDEX
      Next Y
   Next X
   Close 1
   Open Random 1,"Twin:NewLevels.Mst"
   Field 1,2 As NUM$,2 As XD$,2 As YD$,2 As MV$
   INDEX=(N-1)*5
   For I=1 To 5
      Inc INDEX
      Get 1,INDEX
      MN(I)=Val(NUM$)
      MX(I)=Val(XD$)
      MY(I)=Val(YD$)
      MV(I)=Val(MV$)
   Next I
   Close 1
End Proc
Procedure BONUS
   Shared BONUS,LEVEL,CURBONUS
   Every Off 
   Do 
      X=Rnd(13)+1
      Y=Rnd(13)+1
      If C(X,Y)=0
         If Y=14
            Exit 
         Else 
            If C(X,Y+1)=1
               Exit 
            End If 
         End If 
      End If 
   Loop 
   CURBONUS=Rnd(15)+37
   Bob BONUS,X*16,Y*16-1,CURBONUS
   Every On 
End Proc
