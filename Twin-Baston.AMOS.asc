Dim C(14,14)
Dim HISCORE(10)
Dim HISCORE$(10)
Dim MN(5),MX(5),MY(5),MV(5)
Global C()
V$="0.87"
Dim ENNEMI(5),TRESOR(3)
Dim DANSLESPOMMES(5)
'
JOUEUR1=1
JOUEUR2=2
TIR1=3
TIR2=4
ENNEMI(1)=5
ENNEMI(2)=6
ENNEMI(3)=7
ENNEMI(4)=8
ENNEMI(5)=9
TRESOR(1)=10
TRESOR(2)=11
TRESOR(3)=12
BONUS=13
'
Restore SCORES
For I=1 To 10
   Read HISCORE$(I),HISCORE(I)
Next I
SCORES:
Data "POWERMOB PRODUCTIONS",100000
Data "POWERMOB PRODUCTIONS",80000
Data "POWERMOB PRODUCTIONS",70000
Data "POWERMOB PRODUCTIONS",60000
Data "POWERMOB PRODUCTIONS",50000
Data "POWERMOB PRODUCTIONS",40000
Data "POWERMOB PRODUCTIONS",30000
Data "POWERMOB PRODUCTIONS",20000
Data "POWERMOB PRODUCTIONS",10000
Data "POWERMOB PRODUCTIONS",5000
'
MUSIQUEVOLUME=63
BRUITAGESVOLUME=63
ACTIVEMUSIQUE=True
ACTIVEPUB=True
NAME1$="Mr Reed - The Very Best"
NAME2$="Crappy opponent"
'
Proc INIT
Auto View Off 
Wait Vbl 
Screen Open 0,320,256,32,Lowres
Screen Display 0,128,40,320,256
Curs Off 
Flash Off 
Hide On 
Cls 0
Double Buffer 
Auto View On 
Wait Vbl 
Make Mask 
'
'************************************* 
'
Do 
   Autoback 2
   Proc PRESENTATION
   Proc OPTIONS
   '
   Load "Twin:Img-Back.Abk",9
   CREDITS=2
   LEVEL=1
   SCORE1=0
   SCORE2=0
   For I=1 To 5
      DANSLESPOMMES(I)=0
   Next I
   '
   If ACTIVEMUSIQUE
      Proc MUSIQUE[2,0]
   End If 
   Proc _GETREADY
   Proc CHARGER[LEVEL]
   Timer=0
   Repeat 
   Until Fire(1) or Fire(0) or Timer>=400
   Fade 1
   Wait 15
   Bob Off 
   Cls 0
   '
   '****************************************
   '
   Repeat 
      '
      PASSE=False
      MORT1=False
      MORT2=False
      RAMENE1=0
      RAMENE2=0
      CARRY1=0
      CARRY2=0
      PARALIZED1=False
      PARALIZED2=False
      Proc INIT_TABLEAU
      Wait Vbl 
      For I=1 To NBMONSTRES+4
         Amal On I
      Next I
      Synchro Off 
      Timer=0
      '
      Repeat 
         '- - - - - JOUEUR 1 - - - - - -
         If MORT1=False
            If PARALIZED1=False
               Y1=Y Bob(JOUEUR1)
               X1=X Bob(JOUEUR1)
               If Y1>=208
                  Amreg(1,0)=1
                  Amreg(1,1)=1
               Else 
                  X=X1/16
                  Y=Y1/16
                  Add Y,2
                  Amreg(1,0)=C(X,Y)
                  If X<14
                     X=(X1+13)/16
                     Amreg(1,1)=C(X,Y)
                  Else 
                     Amreg(1,1)=0
                  End If 
               End If 
               X=X1/16
               Y=Y1/16
               If X=1
                  Amreg(1,2)=0
               Else 
                  Dec X
                  Inc Y
                  Amreg(1,2)=C(X,Y)
                  Inc X
               End If 
               If X<14
                  Inc X
                  Amreg(1,3)=C(X,Y)
               End If 
               '
               If Bob Col(JOUEUR1,BONUS To BONUS)
                  Proc EFFET_BONUS1
                  Proc BONUS
               End If 
               '
               If Bob Col(JOUEUR1,TRESOR(1) To TRESOR(3))
                  If CARRY1=0
                     If Col(TRESOR(1))
                        CARRY1=1
                     End If 
                     If Col(TRESOR(2))
                        CARRY1=2
                     End If 
                     If Col(TRESOR(3))
                        CARRY1=3
                     End If 
                     Bob Off TRESOR(CARRY1)
                     Proc UPDSCORE[100,0]
                     If ACTIVEMUSIQUE
                        Sam Play %1,2
                     End If 
                  End If 
               End If 
               '
               If X1=16 and Y1=223
                  If CARRY1>0
                     CARRY1=0
                     Inc RAMENE1
                     Proc UPDSCORE[2000,0]
                     If RAMENE1+RAMENE2=3
                        PASSE=True
                     End If 
                     If ACTIVEMUSIQUE
                        Sam Play %1,10
                     End If 
                  End If 
                  If Jdown(1)
                     Inc TEMPS1
                     If TEMPS1>189
                        TEMPS1=189
                     End If 
                     Ink 20
                     TP=206-TEMPS1
                     Draw 1,TP+1 To 14,TP+1
                     Draw 1,TP To 14,TP
                  End If 
               End If 
            Else 
               Inc PARALIZED1
               If PARALIZED1=200
                  PARALIZED1=0
                  Amal Freeze 10
                  Bob JOUEUR1,,,PARIMG1
                  Amal On 1
                  UPDSCORE[0,0]
               End If 
            End If 
            '  à exécuter toujours 
            If Bob Col(TIR1,ENNEMI(1) To ENNEMI(NBMONSTRES))
               For I=ENNEMI(1) To ENNEMI(5)
                  Exit If Col(I)
               Next I
               If DANSLESPOMMES(I-4)=False
                  DANSLESPOMMES(I-4)=1
                  Amal Freeze I
                  Bob TIR1,,320,
                  If ACTIVEMUSIQUE
                     Shoot 
                  End If 
                  Proc UPDSCORE[10,0]
               End If 
            End If 
            If Bob Col(TIR1,JOUEUR2 To JOUEUR2) and PARALIZED2=False
               PARALIZED2=1
               PARIMG2=I Bob(JOUEUR2)
               Amal Freeze 2
               Amal On 11
               Bob TIR1,,320,
               If ACTIVEMUSIQUE
                  Sam Play %10,1,12000
               End If 
               Proc UPDSCORE[50,0]
            End If 
            If Bob Col(JOUEUR1,ENNEMI(1) To ENNEMI(NBMONSTRES))
               MORT1=True
               If ACTIVEMUSIQUE
                  Boom 
               End If 
               Amal 1,"Let A=59; P; P; P; P; P; Let Y=300; P; End"
               Amal On 1
               Bob Off TIR1
               If CARRY1<>0
                  Bob TRESOR(CARRY1),X1,Y1,14
               End If 
               UPDSCORE[0,0]
            End If 
            ' si à 0 
            If TEMPS1=0
               MORT1=True
               If ACTIVEMUSIQUE
                  Sam Play %1,4
               End If 
               Amal 1,"Let A=59; P; P; P; P; P; Let Y=300; P; End"
               Amal On 1
               Bob Off TIR1
               If CARRY1<>0
                  Bob TRESOR(CARRY1),X1,Y1,14
               End If 
               UPDSCORE[0,0]
            End If 
         End If 
         ' - - - - - - JOUEUR 2 - - - - - - - - - 
         If MORT2=False
            If PARALIZED2=False
               Y2=Y Bob(JOUEUR2)
               X2=X Bob(JOUEUR2)
               If Y2>=208
                  Amreg(2,0)=1
                  Amreg(2,1)=1
               Else 
                  X=X2/16
                  Y=Y2/16
                  Add Y,2
                  Amreg(2,0)=C(X,Y)
                  If X<14
                     X=(X2+13)/16
                     Amreg(2,1)=C(X,Y)
                  Else 
                     Amreg(2,1)=0
                  End If 
               End If 
               X=X2/16
               Y=Y2/16
               If X=1
                  Amreg(2,2)=0
               Else 
                  Dec X
                  Inc Y
                  Amreg(2,2)=C(X,Y)
                  Inc X
               End If 
               If X<14
                  Inc X
                  Amreg(2,3)=C(X,Y)
               End If 
               '
               If Bob Col(JOUEUR2,BONUS To BONUS)
                  Proc EFFET_BONUS2
                  Proc BONUS
               End If 
               If Bob Col(JOUEUR2,TRESOR(1) To TRESOR(3))
                  If CARRY2=0
                     If Col(TRESOR(1))
                        CARRY2=1
                     End If 
                     If Col(TRESOR(2))
                        CARRY2=2
                     End If 
                     If Col(TRESOR(3))
                        CARRY2=3
                     End If 
                     Bob Off TRESOR(CARRY2)
                     Proc UPDSCORE[0,100]
                     If ACTIVEMUSIQUE
                        Sam Play %10,2
                     End If 
                  End If 
               End If 
               If(X2=224) and(Y2=223)
                  If CARRY2>0
                     CARRY2=0
                     Inc RAMENE2
                     If RAMENE1+RAMENE2=3
                        PASSE=True
                     End If 
                     Proc UPDSCORE[0,2000]
                     If ACTIVEMUSIQUE
                        Sam Play %10,10
                     End If 
                  End If 
                  If Jdown(0)
                     Inc TEMPS2
                     If TEMPS2>189
                        TEMPS2=189
                     End If 
                     Ink 20
                     TP=206-TEMPS2
                     Draw 241,TP+1 To 254,TP+1
                     Draw 241,TP To 254,TP
                  End If 
               End If 
            Else 
               Inc PARALIZED2
               If PARALIZED2=200
                  PARALIZED2=0
                  Amal Freeze 11
                  Bob JOUEUR2,,,PARIMG2
                  Amal On 2
                  UPDSCORE[0,0]
               End If 
            End If 
            If Bob Col(TIR2,ENNEMI(1) To ENNEMI(NBMONSTRES))
               For I=ENNEMI(1) To ENNEMI(NBMONSTRES)
                  Exit If Col(I)
               Next I
               If DANSLESPOMMES(I-4)=False
                  DANSLESPOMMES(I-4)=1
                  Amal Freeze I
                  Bob TIR2,,320,
                  If ACTIVEMUSIQUE
                     Shoot 
                  End If 
                  Proc UPDSCORE[0,10]
               End If 
            End If 
            If Bob Col(TIR2,JOUEUR1 To JOUEUR1) and(PARALIZED1=False)
               PARALIZED1=1
               Amal Freeze 1
               PARIMG1=I Bob(JOUEUR1)
               Amal On 10
               Bob TIR2,,310,
               If ACTIVEMUSIQUE
                  Sam Play %1,1,6000
               End If 
               Proc UPDSCORE[0,50]
            End If 
            If Bob Col(JOUEUR2,ENNEMI(1) To ENNEMI(NBMONSTRES))
               MORT2=True
               If ACTIVEMUSIQUE
                  Boom 
               End If 
               Amal 2,"Let A=59; P; P; P; P; P; Let Y=300; P; End"
               Amal On 2
               Bob Off TIR2
               If CARRY2>0
                  Bob TRESOR(CARRY2),X2,Y2,14
               End If 
               UPDSCORE[0,0]
            End If 
            ' gestion du temps 
            If TEMPS2<1
               MORT2=True
               If ACTIVEMUSIQUE
                  Sam Play %10,4
               End If 
               Amal 2,"Let A=59; P; P; P; P; P; Let Y=300; P; End"
               Amal On 2
               Bob Off TIR2
               If CARRY2<>0
                  Bob TRESOR(CARRY2),X2,Y2,14
               End If 
               UPDSCORE[0,0]
            End If 
         End If 
         ' - - - - - - - AUTRES GESTIONS - - - - - - - - - - - -
         If Timer>12
            Ink 0
            If MORT1=False
               Dec TEMPS1
               Draw 1,205-TEMPS1 To 14,205-TEMPS1
               Draw 1,206-TEMPS1 To 14,206-TEMPS1
            End If 
            If MORT2=False
               Dec TEMPS2
               Draw 241,205-TEMPS2 To 254,205-TEMPS2
               Draw 241,206-TEMPS2 To 254,206-TEMPS2
            End If 
            Timer=0
         End If 
         For I=ENNEMI(1) To ENNEMI(NBMONSTRES)
            If DANSLESPOMMES(I-4)=200
               DANSLESPOMMES(I-4)=0
               Amal On I
            End If 
            If DANSLESPOMMES(I-4)<>0
               Inc DANSLESPOMMES(I-4)
            End If 
         Next I
         '
         K$=Inkey$
         If K$<>""
            K$=Upper$(K$)
            If Asc(K$)=27
               If Key Shift<>0
                  Erase 2
                  Music Off 
                  Erase 3
                  Erase 5
                  Erase 9
                  Amal Off 
                  Bob Off 
                  Erase 1
                  End 
               Else 
                  MORT1=True
                  MORT2=True
                  CREDITS=1
               End If 
            End If 
            If K$="P"
               Every Off 
               Amal Freeze 
               Repeat 
               Until Upper$(Inkey$)="P"
               For I=1 To 4
                  Amal On I
               Next I
               For I=5 To NBMONSTRES+4
                  If DANSLESPOMMES(I-4)=0
                     Amal On I
                  End If 
               Next I
               Every On 
            End If 
            If K$="M"
               Print At(3,1);"Fast:";Fast Free;" / Chip:";Chip Free;"  "
               Print "R0 =";Amreg(1,0);"  /  R1 =";Amreg(1,1)
            End If 
            If K$="+"
               PASSE=True
            End If 
         End If 
         '
         Synchro 
         Synchro 
         Wait Vbl 
         '
      Until((MORT1=True) and(MORT2=True)) or(PASSE=True)
      '
      '**********************************************
      '
      Every Off 
      Wait 6
      Amal Off 
      MUSIQUE_OFF
      Wait 4
      Fade 2
      Wait 30
      Wait Vbl 
      Bob Off 
      Wait Vbl 
      If PASSE
         If ACTIVEMUSIQUE
            Sam Play %1111,5
         End If 
         If RAMENE1>=RAMENE2
            Proc WINNER[1]
         Else 
            Proc WINNER[2]
         End If 
         If ACTIVEPUB
            Proc PUB[LEVEL]
         End If 
         Inc LEVEL
         _GETREADY
         CHARGER[LEVEL]
         Timer=0
         Repeat 
         Until Fire(1) or Fire(0) or Timer>=400
         Fade 1
         Wait 15
         Cls 0
      Else 
         Dec CREDITS
         If CREDITS>0
            _OH_NO
         End If 
      End If 
   Until CREDITS=0
   ' - - - - - - - - - - - - - - -
   FINBOUCLE:
   Proc _GAME_OVER
   For I=1 To 10
      If SCORE1>HISCORE(I)
         HISCORE(I)=SCORE1
         HISCORE$(I)=NAME1$
         Exit 
      End If 
   Next I
   For I=1 To 10
      If SCORE2>HISCORE(I)
         HISCORE(I)=SCORE2
         HISCORE$(I)=NAME2$
      End If 
   Next I
   Cls 0
   Erase 3
   Erase 5
   HALL_OF_FAME
   Fade 1,0,$FFF
   Wait 15
   Repeat 
   Until Fire(1)=0 and Fire(0)=0
   Repeat 
   Until Fire(1) or Fire(0)
   Fade 1
Loop 
'
'***************** PROCEDURES **************** 
'
'
Procedure INIT
   ' Banques:  1=spr. 2=icons  3=music  5=spl  8=fond  9-10=imgs à charger  
   Shared MUSIQUEON,QUELLEMUSIQUE
   Close Editor 
   Close Workbench 
   While Screen>=0
      Screen Close Screen
   Wend 
   For I=1 To 16
      Erase I
   Next I
   Colour Back 0
   Auto View Off 
   Screen Open 2,320,16,2,Lowres
   Screen Hide 2
   Hide On 
   Proc _COPYRIGHT
   Auto View On 
   Load "Twin:Zik-Intro.Abk",3
   Load "Twin:Img-Intro.Abk",9
   Load "Twin:Samples-First.Abk",5
   Sam Play 1
   Wait 166
   Sam Play 2
   Wait 166
   Sam Play 3
   Wait 114
   Sam Play 4
   Unpack 9 To 0
   Wait Vbl 
   Erase 9
   Wait 80
   Erase 5
   Voice 0
   Mvolume 63
   Voice 15
   Music 1
   MUSIQUEON=True
   QUELLEMUSIQUE=1
   Load "Twin:Sprites.Abk"
   Load "Twin:Icons.Abk",2
End Proc
Procedure PRESENTATION
   Shared MUSIQUEVOLUME,MUSIQUEON,V$
   Screen 0
   Screen To Front 0
   Cls 0
   Load "Twin:Img-Intro2.abk",9
   Load "Twin:Img-Intro3.abk",10
   If MUSIQUEON=False
      Proc MUSIQUE[1,MUSIQUEVOLUME]
   End If 
   Wait Vbl 
   Screen Show 0
   HALL=False
   Do 
      ' Twin Baston
      Auto View Off 
      Wait Vbl 
      Unpack 9 To 2
      Screen 2
      Screen To Front 2
      Screen Display 2,128,40,,
      Palette 0,0
      Auto View On 
      Wait Vbl 
      Fade 1,$0,$FFF
      For I=1 To 250
         If Fire(1) or Fire(0)
            Exit 2
         End If 
         Wait Vbl 
      Next I
      Fade 1
      Wait 15
      ' PowerMob 
      Auto View Off 
      Wait Vbl 
      Unpack 10 To 2
      Palette 0,0
      Auto View On 
      Wait Vbl 
      Fade 1,$0,$FFF
      Screen Display 2,128,40,,
      For I=1 To 250
         If Fire(1) or Fire(0)
            Exit 2
         End If 
         Wait Vbl 
      Next I
      Fade 1
      Wait 15
      ' Scores 
      Screen 0
      Screen To Front 0
      If HALL=False
         Proc HALL_OF_FAME
         HALL=True
      End If 
      Fade 1,0,$FFF
      For I=1 To 250
         If Fire(1) or Fire(0)
            Exit 2
         End If 
         Wait Vbl 
      Next I
      Fade 1
      Wait 15
   Loop 
   '
   Repeat 
   Until Fire(1)=0 and Fire(0)=0
   Fade 1
   Wait 5
   Screen Hide 0
   Screen Hide 2
   Screen Close 2
   Screen 0
   Screen To Front 0
   Flash Off 
   Cls 0
   Screen Show 0
   Proc MUSIQUE_OFF
   Erase 9
   Erase 10
   Erase 3
End Proc
Procedure INIT_TABLEAU
   Shared LEVEL,TEMPS1,TEMPS2,ACTIVEMUSIQUE,MUSIQUEVOLUME,V$,MORT1,MORT2
   Shared ENNEMI(),NBMONSTRES,JOUEUR1,JOUEUR2,TIR1,TIR2,TRESOR()
   Shared MX(),MY(),MN(),MV()
   '
   Auto View Off 
   Screen Open 0,320,256,32,Lowres
   Screen Hide 0
   Curs Off 
   Flash Off 
   Hide On 
   Wait Vbl 
   Restore _TEMPS
   For I=1 To LEVEL
      Read TEMPS1,TEMPS2
   Next I
   _TEMPS:
   Data 190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190
   Data 190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190,190
   '
   '
   Cls 0
   Paper 0
   Pen 1
   Unpack 9 To 1
   Screen 0
   Get Palette 1
   Wait Vbl 
   Screen Copy 1 To 0
   Wait Vbl 
   Screen Close 1
   Screen Display 0,128,40,320,256
   Proc REFOUS_PALETTE
   Ink 20
   Paint 1,16
   Paint 241,16
   Proc UPDSCORE[0,0]
   '
   For CX=1 To 14
      For CY=1 To 14
         If C(CX,CY)=1
            Paste Bob CX*16,CY*16-16,15
         End If 
      Next CY
   Next CX
   Double Buffer 
   Autoback 1
   '
   '
   Bob JOUEUR1,16,223,2
   Bob TIR1,10,320,18
   Bob JOUEUR2,224,223,9
   Bob TIR2,30,320,17
   Limit Bob 16,0 To 240,224
   '
   ' Programmes AMAL
   '
   ' Joueur 1 
   '
   A$="      r zero case en dessous gauche ;"
   A$=A$+"   r un case en dessous droite ;"
   A$=A$+"   r deux case a gauche ;"
   A$=A$+"   r trois case a droite ;"
   A$=A$+"   r quatre empjump ;"
   A$=A$+"   r a  emp tirer"
   A$=A$+"   Let R0=1 ; Let RA=0;"
   A$=A$+"AUtotest("
   A$=A$+"   If R4=0 then Jump Gauche ;"
   A$=A$+"   If J1&1<>0 then Jump Gauche ;"
   A$=A$+"   Let R4=0 ;"
   A$=A$+"Gauche:"
   A$=A$+"   If J1&4=0 then Jump Droite ;"
   A$=A$+"   If X=16 then Jump Droite ;"
   A$=A$+"   Let R5=X/16*16=X ;"
   A$=A$+"   If R2=1&R5 then Jump Droite ;"
   A$=A$+"   Let X=X-1 ; Let A=3 ;"
   A$=A$+"Droite:"
   A$=A$+"   If J1&8=0 then Jump Fire ;"
   A$=A$+"   If R3=1 then Jump Fire ;"
   A$=A$+"   If X=224 then Jump Fire ;"
   A$=A$+"   Let X=X+1 ; Let A=2 ;"
   A$=A$+"Fire:"
   A$=A$+"   If J1&16=0 then eXit;"
   A$=A$+"   If RA=1 then eXit;"
   A$=A$+"   Let RA=1; Let RB=1;"
   A$=A$+"   Let RC=X; Let RD=Y;"
   A$=A$+"   Let RE=A;"
   A$=A$+"   eXit; )"
   A$=A$+"Sauter:"
   A$=A$+"   If R4=1 then Jump Tomber ;"
   A$=A$+"   If J1&1=0 then Jump Tomber ;"
   A$=A$+"   If Y<30 then Jump Tomber ;"
   A$=A$+"   Let R4=1 ;"
   A$=A$+"   Move 0,-32,16 ; Pause; Pause;"
   A$=A$+"Tomber:"
   A$=A$+"   If R0=1 then Jump Sauter ;"
   A$=A$+"   If R1=1 then Jump Sauter ;"
   A$=A$+"   Move 0,16,8 ;"
   A$=A$+"   Jump Tomber ;"
   '
   ' Tir 1
   '
   C$="   Loop:"
   C$=C$+"   If RB=0 then Jump Loop;"
   C$=C$+"   Let RB=0;"
   C$=C$+"   Let X=RC; Let Y=RD;"
   C$=C$+"   If RE=2 then Jump M;"
   C$=C$+"   Let A=17;"
   C$=C$+"   Move -48,0,12;"
   C$=C$+"   Jump N;"
   C$=C$+"M: Let A=18"
   C$=C$+"   Move 48,0,12;"
   C$=C$+"N: Let Y=310;"
   C$=C$+"   Let RA=0"
   C$=C$+"   Jump Loop;"
   '
   ' Joueur 2 
   '
   B$="      r zero case en dessous gauche ;"
   B$=B$+"   r un case en dessous droite ;"
   B$=B$+"   r deux case a gauche ;"
   B$=B$+"   r trois case a droite ;"
   B$=B$+"   r quatre et r cinq empjump ;"
   B$=B$+"   Let R0=1 ;"
   B$=B$+"AUtotest("
   B$=B$+"   If R4=0 then Jump Gauche ;"
   B$=B$+"   If J0&1<>0 then Jump Gauche ;"
   B$=B$+"   Let R4=0 ;"
   B$=B$+"Gauche:"
   B$=B$+"   If J0&4=0 then Jump Droite ;"
   B$=B$+"   If X=16 then Jump Droite ;"
   B$=B$+"   Let R5=X/16*16=X ;"
   B$=B$+"   If R2=1&R5 then Jump Droite ;"
   B$=B$+"   Let X=X-1 ; Let A=9 ;"
   B$=B$+"Droite:"
   B$=B$+"   If J0&8=0 then Jump Fire ;"
   B$=B$+"   If R3=1 then Jump Fire ;"
   B$=B$+"   If X=224 then Jump Fire ;"
   B$=B$+"   Let X=X+1 ; Let A=8 ;"
   B$=B$+"Fire:"
   B$=B$+"   If J0&16=0 then eXit;"
   B$=B$+"   If RA=1 then eXit;"
   B$=B$+"   Let RF=1; Let RG=1;"
