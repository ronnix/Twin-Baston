Procedure GAME
   Shared MSG_SELECT
   Cls 0
   Palette 0,0,0,0,0
   _MESSAGE[MSG_SELECT,0]
   R=1
   DRSEL2[R]
   Fade 1,0,$FFF,$AAA,$555,$FFF
   Wait 15
   Flash 4,"(FFF,5)(000,5)"
   Repeat 
      If(Jup(1)) or(Key State(76))
         Add R,-1,1 To 7
         DRSEL2[R]
         While(Jup(1)) or(Key State(76))
         Wend 
      End If 
      If(Jdown(1)) or(Key State(77))
         Add R,1,1 To 7
         DRSEL2[R]
         While(Jdown(1)) or(Key State(77))
         Wend 
      End If 
   Until(Fire(1)) or(Key State(64))
   Flash Off 
   Fade 1
   Wait 15
   Cls 0
   On R Proc JEU4,JEU3,JEU2,JEU1,JEU6,JEU7,JEU5
End Proc
Procedure DRSEL2[N]
   Ink 1+(N=1)*(-3),0
   Text 124,100,"PING-PONG"
   Ink 1+(N=2)*(-3),0
   Text 136,120,"TUTURE"
   Ink 1+(N=3)*(-3),0
   Text 116,140,"SECOUEZ-MOI"
   Ink 1+(N=4)*(-3),0
   Text 136,160,"T.L.C."
   Ink 1+(N=5)*(-3),0
   Text 132,180,"PAK MAN"
   Ink 1+(N=6)*(-3),0
   Text 100,200,"LEMMINGSATOR II"
   Ink 1+(N=7)*(-3),0
   Text 92,220,"STRIP FIGHTER III"
End Proc
Procedure JEU[N]
   If N<=4
      On N Proc JEU1,JEU3,JEU2,JEU4
   Else 
      N=(N mod 5)+1
      On N Proc JEU4,JEU7,JEU6,JEU5,JEU3
   End If 
End Proc
Procedure JEU1
   ' Clone de Tron light cycles 
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,TBLB
   Volume JEUXVOL
   Palette 0,$F,$F00,$FFF
   Paper 0
   Pen 3
   Cls 0
   Centre At(,0)+"EVITEZ LES COLLISIONS !"
   Centre At(,2)+"BONUS 10.000 Pts"
   Autoback 1
   Bob Update Off 
   Ink 3,0
   Box 0,32 To 319,255
   X1=32
   Y1=64
   S1=8
   P1=0
   X2=280
   Y2=200
   S2=4
   P2=0
   Repeat 
      If Jup(1) and S1<>2
         S1=1
      End If 
      If Jdown(1) and S1<>1
         S1=2
      End If 
      If Jleft(1) and S1<>8
         S1=4
      End If 
      If Jright(1) and S1<>4
         S1=8
      End If 
      If S1=1
         Dec Y1
      End If 
      If S1=2
         Inc Y1
      End If 
      If S1=4
         Dec X1
      End If 
      If S1=8
         Inc X1
      End If 
      If Point(X1,Y1)>0
         FIN=True
         P1=True
      End If 
      If((Jup(0)=True) or(Key State(76)=True)) and(S2<>2)
         S2=1
      End If 
      If((Jdown(0)=True) or(Key State(77)=True)) and(S2<>1)
         S2=2
      End If 
      If((Jleft(0)=True) or(Key State(79)=True)) and(S2<>8)
         S2=4
      End If 
      If((Jright(0)=True) or(Key State(78)=True)) and(S2<>4)
         S2=8
      End If 
      If S2=1
         Dec Y2
      End If 
      If S2=2
         Inc Y2
      End If 
      If S2=4
         Dec X2
      End If 
      If S2=8
         Inc X2
      End If 
      If Point(X2,Y2)>0
         FIN=True
         P2=True
      End If 
      Plot X1,Y1,1
      Plot X2,Y2,2
      Wait Vbl 
   Until FIN
   Boom 
   Bob Update On 
   Fade 1
   Wait 15
   Cls 0
   SPEND
   SP[2]
   If(P1=True) and(P2=True)
      _MESSAGE[MSG_EGALITE,0]
      Add SCORE(1),5000
      Add SCORE(2),5000
   Else 
      TBLB=True
      If P2=True
         _MESSAGE[MSG_WINNER,1]
         Add SCORE(1),10000
      Else 
         _MESSAGE[MSG_WINNER,2]
         Add SCORE(2),10000
      End If 
   End If 
   TBLB=False
   Volume 0
End Proc
Procedure JEU2
   ' Le jeu le plus con: Secouez-moi
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,TBLB
   Volume JEUXVOL
   Palette 0,$F,$F00,$FFF
   Paper 0
   Pen 3
   Autoback 1
   Centre At(,3)+"SECOUEZ-MOI DE GAUCHE A DROITE !"
   Centre At(,5)+"BONUS 10.000 Pts"
   Ink 3,0
   Bar 160,96 To 161,130
   T=499
   J1=8
   J2=8
   G=0
   Repeat 
      J=Joy(1)
      If((J=4) and(J1=8)) or((J=8) and(J1=4))
         J1=J
         G=Min(128,G+2)
      End If 
      J=Joy(0)
      If(Key State(79)=True) and(Key State(78)=False)
         J=4
      End If 
      If(Key State(78)=True) and(Key State(79)=False)
         J=8
      End If 
      If((J=4) and(J2=8)) or((J=8) and(J2=4))
         J2=J
         G=Max(-128,G-2)
      End If 
      Ink 1,0
      Bar 30,100 To 160+G,126
      Ink 2,0
      Bar 160+G+1,100 To 290,126
      Ink 3,0
      If(T mod 50)=0
         Text 120,180,"TEMPS ="+Str$(T/50-1)+" "
         Bell T/25+10
      End If 
      Wait Vbl 
      Dec T
   Until T=0
   Fade 1
   Wait 16
   Cls 0
   SPEND
   SP[4]
   TBLB=True
   If G=0
      _MESSAGE[MSG_EGALITE,0]
      Add SCORE(1),5000
      Add SCORE(2),5000
   Else 
      If G>0
         _MESSAGE[MSG_WINNER,1]
         Add SCORE(1),10000
      Else 
         _MESSAGE[MSG_WINNER,2]
         Add SCORE(2),10000
      End If 
   End If 
   TBLB=False
   Volume 0
End Proc
Procedure JEU3
   ' Jeu de tuture pour TB ( Eric vs Boisroux II )  
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,TBLB
   Dim P(31)
   For I=0 To 31
      P(I)=Colour(I)
   Next I
   L1=Length(1)
   _LOAD["Twin2:Sprites-Tuture.Abk",1]
   Get Sprite Palette 
   Priority Reverse On 
   C=0
   For I=19 To 23
      Colour I,$FFF
   Next I
   Colour 16,$555
   Colour 17,$555
   Colour 18,$555
   '
   _LOAD["Twin2:Samples-Tuture.Abk",6]
   Sam Bank 6
   Volume JEUXVOL
   Sam Play 2
   '
   Amal Off 
   Bob Off 
   Bob Clear 
   Wait Vbl 
   Cls 0
   Screen Swap 
   Bob Clear 
   Wait Vbl 
   Cls 0
   Ink 16
   Bar 40,0 To 280,256
   For Y=0 To 256 Step 4
      Add C,1,17 To 23
      Ink C
      Bar 150,Y To 158,Y+3
   Next Y
   Ink 1,16
   Text 48,7,"PLAYER 1 = 0"
   Text 170,7,"PLAYER 2 = 0"
   Shift Up 2,17,23,1
   '
   Autoback 1
   Synchro On 
   '
   Dim X(1),Y(1)
   X(1)=60
   Y(1)=160
   X(0)=220
   Y(0)=160
   BOISROUX=0
   SPLATCH1=0
   SPLATCH2=0
   GAGNE=False
   Randomize Timer
   Repeat 
      If BOISROUX=0
         Bob 3,Rnd(200)+48,0,Vrev(L1+3)
         Channel 1 To Bob 3
         Amal 3,("Anim0,("+Str$(Vrev(L1+3))+",10)("+Str$(Vrev(L1+4))+",10);")-" "
         Amal On 3
         EU=False
      End If 
      If BOISROUX>=256
         BOISROUX=-3
      End If 
      Add BOISROUX,3
      C=Bob Col(1,2 To 2)
      If EU=False
         BX=Bob Col(3,1 To 2)
         If BX=True
            Sam Play 1
            Amal Freeze 
            Bob 3,,,Vrev(L1+5)
            EU=True
            If Col(1)=True
               Inc SPLATCH1
               If SPLATCH1=8
                  GAGNE=1
               End If 
            Else 
               Inc SPLATCH2
               If SPLATCH2=8
                  GAGNE=2
               End If 
            End If 
            Text 48,7,"PLAYER 1 ="+Str$(SPLATCH1)
            Text 170,7,"PLAYER 2 ="+Str$(SPLATCH2)
         End If 
      End If 
      If C
         If X(1)<X(0)
            CX=1
         Else 
            CX=0
         End If 
         If Y(1)<Y(0)
            CY=1
         Else 
            CY=0
         End If 
      End If 
      For I=0 To 1
         If((Jleft(I)=True) or((Key State(79)=True) and(I=0))) and(X(I)>44)
            If(C=False) or((C=True) and(CX=I))
               Add X(I),-4
            End If 
         End If 
         If((Jright(I)=True) or((Key State(78)=True) and(I=0))) and(X(I)<246)
            If(C=False) or((C=True) and(CX<>I))
               Add X(I),4
            End If 
         End If 
         If((Jup(I)=True) or((Key State(76)=True) and(I=0))) and(Y(I)>100)
            If(C=False) or((C=True) and(CY=I))
               Add Y(I),-2
            End If 
         End If 
         If((Jdown(I)=True) or((Key State(77)=True) and(I=0))) and(Y(I)<210)
            If(C=False) or((C=True) and(CY<>I))
               Add Y(I),2
            End If 
         End If 
      Next I
      Bob 1,X(1),Y(1),L1+1
      Bob 2,X(0),Y(0),L1+2
      Bob 3,,BOISROUX,
      Wait Vbl 
   Until GAGNE>0
   Sam Play 2
   Shift Off 
   Amal Off 
   A$="Move 0,-280,70;"
   For I=1 To 2
      Channel I To Bob I
      Amal I,A$
   Next I
   Amal On 
   Wait 40
   Fade 1
   Bob Clear 
   Amal Off 
   Bob Off 
   Wait Vbl 
   Cls 0
   Screen Swap 
   Cls 0
   Del Sprite L1+1 To Length(1)
   Volume 0
   Erase 6
   Sam Bank 5
   SP[4]
   TBLB=True
   If GAGNE=1
      _MESSAGE[MSG_WINNER,1]
      Add SCORE(1),10000
   Else 
      _MESSAGE[MSG_WINNER,2]
      Add SCORE(2),10000
   End If 
   TBLB=False
   Wait 20
   Volume 0
   Synchro Off 
End Proc
Procedure JEU4
   ' Super-Tennis 1982
   Shared MSG_EGALITE,MSG_WINNER,SCORE(),JEUXVOL,TBLB
   
   Dim XR(1),YR(1)
   
   _LOAD["Twin2:Samples-PingPong.Abk",6]
   Sam Bank 6
   Volume JEUXVOL
   
   Palette 0,$FFF
   Paper 0
   Pen 1
   
   SC1=0
   SC2=0
   
   JEU=0
   Do 
      Inc JEU
      
      Cls 0
      Centre At(,1)+"SUPER TENNIS 1982"
      Print 
      Print "JOUEUR 1 =";SC1;At(28,);"JOUEUR 2 =";SC2
      Ink 1,0
      Box 0,24 To 319,255
      
      Exit If JEU=6
      
      XR(1)=1
      XR(0)=38
      YR(1)=14
      YR(0)=14
      BX=10+Rnd(16)
      BY=6+Rnd(16)
      VX=Rnd(1)*2-1
      VY=Rnd(1)*2-1
      BOUM=False
      
      Bar XR(1)*8,YR(1)*8 To XR(1)*8+7,(YR(1)+5)*8
      Bar XR(0)*8,YR(0)*8 To XR(0)*8+7,(YR(0)+5)*8
      Bar BX*8,BY*8 To BX*8+7,BY*8+7
      
      Wait 40
      Sam Play 2
      Wait 60
      
      REBOND=0
      TMPO=4
      
      Repeat 
         
         ' Raquettes
         For I=0 To 1
            If((Jup(I)) or((Key State(76)=True) and(I=0))) and(YR(I)>4)
               Ink 0,0
               Bar XR(I)*8,YR(I)*8 To XR(I)*8+7,(YR(I)+5)*8
               Dec YR(I)
            End If 
            If((Jdown(I)) or((Key State(77)=True) and(I=0))) and(YR(I)<26)
               Ink 0,0
               Bar XR(I)*8,YR(I)*8 To XR(I)*8+7,(YR(I)+5)*8
               Inc YR(I)
            End If 
            Ink 1,0
            Bar XR(I)*8,YR(I)*8 To XR(I)*8+7,(YR(I)+5)*8
         Next I
         
         ' Baballe
         Ink 0,0
         Bar BX*8,BY*8 To BX*8+7,BY*8+7
         Add BX,VX
         Add BY,VY
         If(BY=4) or(BY=30)
            VY=-VY
            Sam Play 1
         End If 
         If BX<=2
            If(BY>=(YR(1)-1)) and(BY<=YR(1)+5)
               VX=-VX
               Sam Play 1
               Inc REBOND
            Else 
               BOUM=True
               Inc SC2
            End If 
         End If 
         If BX>=37
            If(BY>=YR(0)-1) and(BY<=YR(0)+5)
               VX=-VX
               Sam Play 1
               Inc REBOND
            Else 
               BOUM=True
               Inc SC1
            End If 
         End If 
         Ink 1,0
         Bar BX*8,BY*8 To BX*8+7,BY*8+7
         If REBOND=4
            REBOND=0
            Dec TMPO
            If TMPO=0
               TMPO=1
            End If 
         End If 
         
         ' 120 MIPS 
         Wait TMPO
         
      Until BOUM
      
      Sam Play 3
      Wait 100
      
   Loop 
   
   Wait 20
   Fade 1
   Wait 15
   Cls 0
   Erase 6
   Sam Bank 5
   SP[2]
   TBLB=True
   If SC1>SC2
      _MESSAGE[MSG_WINNER,1]
      Add SCORE(1),10000
   Else 
      _MESSAGE[MSG_WINNER,2]
      Add SCORE(2),10000
   End If 
   TBLB=False
   Volume 0
End Proc
